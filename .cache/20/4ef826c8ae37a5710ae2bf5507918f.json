).hover(\r\n        function(){\r\n            $('.hero-text').css(\"opacity\", \"0.1\");\r\n        },\r\n        function() {\r\n            $('.hero-text').css(\"opacity\", \"1\");\r\n    });\r\n\r\n}"},"lineCount":null}},"error":null,"hash":"8641e31771ccd799d2c97ddb8549e036","cacheData":{"env":{}}}